#include <__type_traits/__is_union.h>
#include <__type_traits/__is_class.h>
#include <__type_traits/__is_function.h>
#include <__type_traits/__is_pointer.h>
#include <__type_traits/__is_lvalue_reference.h>
#include <__type_traits/__is_rvalue_reference.h>
#include <__type_traits/__is_member_object_pointer.h>
#include <__type_traits/__is_member_function_pointer.h>

// Composite type categories


#include <__type_traits/__is_member_pointer.h>

